<?xml version="1.0" encoding="UTF-8"?>
<colorrules name="[Default]">
  <colorrule name="Compressed" text="#251b98" background="-1" forcedfontstyle="i" enable="1">
    <filter>
      <rule field="Attribute" matchtype="Has" value="C"/>
    </filter>
  </colorrule>
  <colorrule name="System Files" text="#e15f19" background="-1" forcedfontstyle="i" enable="1">
    <filter>
      <rule field="Attribute" matchtype="Has" value="HS"/>
    </filter>
  </colorrule>
  <colorrule name="New File/Folder from today" text="#0310ac" background="-1" forcedfontstyle="b" enable="1">
    <filter>
      <rule field="Date" matchtype="Is" value="Today"/>
    </filter>
  </colorrule>
  <colorrule name="Empty file" text="#d20eee" background="-1" forcedfontstyle="b" enable="1">
    <filter>
      <rule field="Attribute" matchtype="HasNot" value="D"/>
      <rule field="Size" matchtype="Is" value="0"/>
    </filter>
  </colorrule>
  <colorrule name="Executables" text="#18aef7" background="-1" enable="1">
    <filter>
      <rule field="Ext" matchtype="Is" value="exe|bat"/>
    </filter>
  </colorrule>
  <colorrule name="Text documents" text="#2f9f26" background="-1" enable="1">
    <filter>
      <rule field="Ext" matchtype="Is" value="txt|xml|ini|log"/>
    </filter>
  </colorrule>
  <colorrule name="Pictures" text="#fb992e" background="-1" enable="1">
    <filter>
      <rule field="Ext" matchtype="Is" value="jpg|png|bmp|gif"/>
    </filter>
  </colorrule>
  <colorrule name="Video" text="#6ac0b3" background="-1" enable="1">
    <filter>
      <rule field="Ext" matchtype="Is" value="avi|mkv|mov|wmv|mp4"/>
    </filter>
  </colorrule>
  <colorrule name="Music" text="#bb37a7" background="-1" enable="1">
    <filter>
      <rule field="Ext" matchtype="Is" value="mp3|ogg|wav"/>
    </filter>
  </colorrule>
  <colorrule name="Archives" text="#c98831" background="-1" enable="1">
    <filter>
      <rule field="Ext" matchtype="Is" value="zip|rar|tar|gz|7z"/>
    </filter>
  </colorrule>
  <colorrule name="PDF" text="#ec0409" background="-1" forcedfontstyle="b" enable="1">
    <filter>
      <rule field="Ext" matchtype="Is" value="pdf"/>
    </filter>
  </colorrule>
  <colorrule name="Office files" text="#f64c5d" background="-1" enable="1">
    <filter>
      <rule field="Ext" matchtype="Is" value="doc|docx|xls|xlsx"/>
    </filter>
  </colorrule>
</colorrules>
