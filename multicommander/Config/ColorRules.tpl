<?xml version="1.0" encoding="UTF-8"?>
<colorrules name="[Default]">
  <colorrule name="Executables" text="#17a1c9" background="-1" forcedfontstyle="b" enable="1">
    <filter>
      <rule field="Ext" matchtype="Is" value="exe|bat"/>
    </filter>
  </colorrule>
  <colorrule name="Compressed" text="#71b474" background="-1" forcedfontstyle="i" enable="1">
    <filter>
      <rule field="Attribute" matchtype="Has" value="C"/>
    </filter>
  </colorrule>
  <colorrule name="System Files" text="#ac4149" background="-1" enable="1">
    <filter>
      <rule field="Attribute" matchtype="Has" value="S"/>
    </filter>
  </colorrule>
  <colorrule name="New File/Folder from today" text="#0bd90b" background="-1" forcedfontstyle="b" enable="1">
    <filter>
      <rule field="Date" matchtype="Is" value="Today"/>
    </filter>
  </colorrule>
  <colorrule name="Empty file" text="#a7adae" background="-1" enable="1">
    <filter>
      <rule field="Attribute" matchtype="HasNot" value="D"/>
      <rule field="Size" matchtype="Is" value="0"/>
    </filter>
  </colorrule>
  <colorrule name="Text documents" text="#42908a" background="-1" enable="1">
    <filter>
      <rule field="Ext" matchtype="Is" value="txt|xml|ini|log"/>
    </filter>
  </colorrule>
  <colorrule name="Pictures" text="#bb46d4" background="-1" enable="1">
    <filter>
      <rule field="Ext" matchtype="Is" value="jpg|png|bmp|gif"/>
    </filter>
  </colorrule>
  <colorrule name="Music" text="#e37f91" background="-1" enable="1">
    <filter>
      <rule field="Ext" matchtype="Is" value="mp3|ogg|wav"/>
    </filter>
  </colorrule>
  <colorrule name="Video" text="#6ac0b3" background="-1" enable="1">
    <filter>
      <rule field="Ext" matchtype="Is" value="avi|mkv|mov|wmv|mp4"/>
    </filter>
  </colorrule>
  <colorrule name="Archives" text="#d5d016" background="-1" enable="1">
    <filter>
      <rule field="Ext" matchtype="Is" value="zip|rar|tar|gz|7z"/>
    </filter>
  </colorrule>
  <colorrule name="Office files" text="#a7c56a" background="-1" enable="1">
    <filter>
      <rule field="Ext" matchtype="Is" value="doc|docx|xls|xlsx"/>
    </filter>
  </colorrule>
</colorrules>
